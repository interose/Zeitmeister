        {% endblock %}
    </head>
    <body class="bg-gray-50 dark:bg-gray-800 h-full">
        {% include '_top_navigation.html.twig' %}

        <div class="h-full pt-14 flex container flex-col">
            {% block body %}{% endblock %}
        </div>
    </body>
</html>
